				print("ã‚«ãƒ¡ãƒ©ã‚’è‡ªåˆ†ã«æˆ»ã—ã¾ã—ãŸã€‚")
			end
			-- ã‚¤ãƒ™ãƒ³ãƒˆã®æ¥ç¶šã‚’è§£é™¤
			if viewConnection then
				viewConnection:Disconnect()
				viewConnection = nil
			end
		end
	end
})

