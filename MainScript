local viewConnection = nil -- CharacterAddedã®æ¥ç¶šã‚’ä¿å­˜ã—ã¦ãŠãå¤‰æ•°

TeleportTab:AddToggle({
	Name = "View",
	Default = false,
	Callback = function(state)
		local camera = Workspace.CurrentCamera
